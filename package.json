    "security-check": "nsp check",
    "lint": "esw app.js --color",
    "lint:watch": "npm run lint -- --watch"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "chalk": "^2.4.1",
    "dotenv": "^5.0.1",
    "email-validator": "^2.0.4",
    "express": "^4.16.3",
    "jsonwebtoken": "^8.2.2",
    "moment": "^2.22.1",
    "mysql": "^2.15.0",
    "path": "^0.12.7",
    "uniqid": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-plugin-import": "^2.11.0",
    "eslint-watch": "^3.1.4",
    "mocha": "^5.1.1",
    "npm-run-all": "^4.1.3",
    "sinon": "^5.0.10"
  }
